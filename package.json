{
  "name": "termicons",
  "description": "Material style icons optimized for terminal rendering",
  "fontVersion": "1.0",
  "type": "module",
  "scripts": {
    "build": "node ./scripts/build.js",
    "dev": "nodemon -e js,svg,hbs,json -i dist/ ./scripts/build.js --dev",
    "sync": "./scripts/sync.sh",
    "css": "tailwindcss -i ./src/template/styles.css -o ./dist/termicons.css",
    "format": "prettier --write '**/*.{js,svg,hbs,yml,md,cjs,json,css}'"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "fantasticon": "^2.0.0",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "eslint": "8.34.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4"
  }
}
